{
    "extends": "gts/tsconfig-google.json",
    "compilerOptions": {
        "target": "es2017",
        "outDir": "dist",
        "rootDir": "src",
        "lib": ["dom", "es2017"],
        "types": ["node"],
        "jsx": "react",
        "sourceMap": false,
        "declaration": false,
        "moduleResolution": "node"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}
