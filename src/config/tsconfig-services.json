{
    "compilerOptions": {
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "pretty": true,
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "lib": ["dom", "es2017"],
        "types": ["node"],
        "jsx": "react",
        "declaration": false,
        "moduleResolution": "node"
    }
}
